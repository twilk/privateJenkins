  agent any
  
  stages{
    
    stage("build"){
      
      steps {
        echo 'build'
        echo 'test'
        echo 'built'
      }
    }
    stage("test"){
      
      steps {
        echo 'test'
      }
    }
    stage("deploy"){
      
      steps {
        echo 'deploy'
      }
    }
  }
}
